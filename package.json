{
  "name": "@sukryu/aethel-ts",
  "version": "0.1.0",
  "description": "High-performance, type-safe data structure and algorithm library for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:types": "tsc -p tsconfig.build.json",
    "build:js": "node build.mjs",
    "build": "npm run clean && npm run type-check && npm run build:js && npm run build:types",
    "build:prod": "npm run clean && tsc -p tsconfig.build.json --noEmit && node build.mjs --production && npm run build:types",
    "type-check": "tsc --noEmit",
    "bundle": "node build.mjs",
    "bundle:prod": "node build.mjs --production",
    "dev": "node build.mjs --watch",
    "benchmark": "tsx benchmark/list.bench.ts",
    "benchmark:cache": "tsx benchmark/cache.bench.ts",
    "benchmark:all": "npm run benchmark && npm run benchmark:cache",
    "benchmark:compiled": "npm run build && node dist/benchmark/list.bench.js",
    "prepublishOnly": "npm run build:prod",
    "test": "echo \"Warning: No tests specified yet\" && exit 0"
  },
  "keywords": [
    "data-structures",
    "algorithms",
    "typescript",
    "performance",
    "linked-list",
    "lru-cache",
    "cache",
    "queue",
    "tree",
    "high-performance",
    "type-safe"
  ],
  "author": "sukryu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sukryu/aethel-ts.git"
  },
  "bugs": {
    "url": "https://github.com/sukryu/aethel-ts/issues"
  },
  "homepage": "https://github.com/sukryu/aethel-ts#readme",
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@types/node": "^20.10.0",
    "benchmark": "^2.1.4",
    "esbuild": "^0.19.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}